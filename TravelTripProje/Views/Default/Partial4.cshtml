@using TravelTripProje.Models.Siniflar
@model List<Blog>
@{
    Layout = null;
}
<div class="padding1">
    <h4>En Beğendiğim Yerler</h4>
    <div class="box-left">
        @foreach (var item in Model)
        {
            <div class="bot-img"  alt="">
                <img  style="width:100px; height:50px;"src="@item.BlogImage" alt="" />
            </div>
            <div class="bot-desc">
                <p>@item.Aciklama.Substring(0,75)</p>
            </div>
            <div class="clear"></div>
        }
    </div>
    @Html.Action("Partial5","Default")

 
</div>